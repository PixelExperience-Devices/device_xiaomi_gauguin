# Smart charging
allow system_server sysfs_battery_supply:file { getattr open read write };
allow system_server sysfs_usb_supply:file { getattr open read write };

# allow system_server to get vendor_camera_prop
get_prop(system_server, vendor_persist_camera_prop)

# Allow system_server to read Fast Charging status
allow system_server vendor_sysfs_battery_supply:file { getattr open read };
